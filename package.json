    "babel-jest": "^22.0.4",
    "babel-loader": "^9.1.2",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-plugin-module-resolver": "^3.0.0",
    "babel-preset-vue-app": "^2.0.0",
    "babel-preset-vue2": "^1.0.1",
    "histoire": "^0.17.0",
    "jest": "^22.0.4",
    "jest-serializer-html": "^5.0.0",
    "jest-serializer-vue": "^0.3.0",
    "jest-vue-preprocessor": "^1.3.1",
    "jsdom": "^23.0.0",
    "vitest": "^0.34.6",
