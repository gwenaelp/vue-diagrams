  },
  "homepage": "https://github.com/gwenaelp/vue-diagrams#readme",
  "keywords": [
    "vue",
    "vue-component",
    "vue-library"
  ],
  "engines": {
    "node": ">=8.9.0"
  },
  "scripts": {
    "build": "webpack --config webpack.config.js",
    "story:dev": "histoire dev",
    "story:build": "histoire build",
    "doc:build": "cd doc && yarn && yarn generate && rm ./dist -Rf && mkdir ./dist && mv .output/public/* ./dist && touch dist/.nojekyll && cd .. && yarn story:build",
    "test": "jest --verbose --coverage",
    "vitest": "vitest",
    "test:update": "jest --verbose --updateSnapshot",
    "test:watch": "jest --verbose --watchAll --notify",
    "type-check": "vue-tsc --noEmit -p tsconfig.json"
  },
  "devDependencies": {
    "@histoire/plugin-vue2": "^0.15.8",
    "@vue/test-utils": "^2.4.2",
